{
  "manifest_version": 2,

  "name": "UGCRS",
  "description": "La vida esta loca, so are my salads",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "UGCRS"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": ["https://www.urbangardencafe.fr/*"],
    "js": ["jquery.js", "ugcrs.js"]
  }],
  "browser_action": {}
}